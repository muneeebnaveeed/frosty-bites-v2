{
  "name": "diamond-tiles-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 react-scripts start",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "styles": "tailwindcss build -i ./src/css/tailwind.css -o ./src/css/tailwind.build.css --watch",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^2.5.8",
    "ahooks": "^2.10.9",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.6",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "formik": "^2.2.9",
    "html-to-draftjs": "^1.5.0",
    "immer": "^9.0.5",
    "lodash": "^4.17.21",
    "metismenujs": "^1.2.1",
    "nouislider": "^15.2.0",
    "nouislider-react": "^3.3.9",
    "qs": "^6.10.1",
    "react": "^17.0.1",
    "react-avatar": "^3.10.0",
    "react-bootstrap": "^1.4.0",
    "react-date-range": "^1.1.3",
    "react-datepicker": "^4.2.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-if": "^4.0.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-query": "^3.19.1",
    "react-redux": "^7.2.4",
    "react-resize-detector": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^3.1.1",
    "react-to-print": "^2.14.10",
    "react-toastify": "^8.2.0",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "sweetalert": "^2.1.2",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "max-classes-per-file": 0,
      "no-param-reassign": 0,
      "prefer-promise-reject-errors": 1,
      "eqeqeq": 1,
      "no-new": 0,
      "no-console": 1,
      "no-plusplus": 0,
      "react/prop-types": 0,
      "react/jsx-props-no-spreading": 0,
      "react/destructuring-assignment": 1,
      "import/no-unresolved": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/role-supports-aria-props": 0,
      "jsx-a11y/img-redundant-alt": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/label-has-associated-control": 0,
      "jsx-a11y/alt-text": 0,
      "jsx-a11y/control-has-associated-label": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/no-danger": 0,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 120,
          "endOfLine": "auto",
          "tabWidth": 3
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "postcss": "^7.0.39",
    "prettier": "^2.8.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  }
}
